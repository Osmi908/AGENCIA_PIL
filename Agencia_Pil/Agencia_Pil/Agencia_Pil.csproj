      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\PrincipalPageDetail.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
